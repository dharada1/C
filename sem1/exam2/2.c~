#include <stdio.h>
#define N 12

double average(double x[], int size){
  int i;
  double s;
  s=0;
  for(i=0;i<12;i++){
    s=s+x[i];
  }
    return s/size;
}

int main(void){
  double x[] = {-4.1, -3.5, 0.1, 6.7, 12.1, 16.3, 20.5, 22.0, 17.6, 11.3, 4.6, -1.0};
  
  printf("average = %lf\n", average(x, N));

  
  return 0;
}

#include<stdio.h>
#include<math.h>
#define hantei 0.00000001

double func(double a){
  return a*a*a - 6*a*a + 9*a - 1;
}
double dfunc(double a){
  return 3*a*a - 12*a + 6;
}


int main(void){
  double x;
  x=5;
  do{
    x=x[0]-func(x[0])/dfunc(x[0]);
  }while(fabs(func(x))>=hantei);
  printf("%.9f\n",x);
  

  return 0;
}

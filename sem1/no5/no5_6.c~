#include <stdio.h>
int main(void)
{
  int p,q;
  scanf("%d %d",&p,&q);
  
  do{
    if(p=0||q=0){
      break;
    }
  if(p>q){
    p=p-q;
  }
  else if(p<q){
    q=q-p;
  }
  else if(p=q){
    break;
  }
  }while(p>=0||q>=0);

  if(p>0){
    printf("%d",q);
  }
  else if(q>0){
    printf("%d",p);
  }
return 0;
}
